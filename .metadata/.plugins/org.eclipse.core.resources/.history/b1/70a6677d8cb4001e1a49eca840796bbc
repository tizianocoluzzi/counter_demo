package counter_demo;

public class Counter {
	private int count;
	public Counter(int count) {
		this.count = count;
	}
	public Counter() {
		this.count = 0;
	}
	public void incCount() {
		this.count += 1;
	}
	public void decCount() {
		this.count -= 1;
	}
	public int getCount() {
		return count;
	}
}
